<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/linearlayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="200px">
        <TextView
            android:id="@+id/hide"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textColor="@color/black"
            android:textSize="1dp"/>
        <TextView
            android:id="@+id/resule_text"
            android:layout_width="440px"
            android:layout_height="140px"
            android:text="调试参数"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="100px"/>

    </RelativeLayout>

    <RelativeLayout
        android:background="@drawable/the_shape2"
        android:padding="20px"
        android:layout_width="match_parent"
        android:layout_height="1400px">

        <TextView
            android:id="@+id/textView3"
            style="@style/second_normalTextView"
            android:layout_marginTop="120px"
            android:layout_marginLeft="110px"
            android:text="直流Ud"
        />
        <TextView
            android:id="@+id/textView4"
            android:layout_below="@+id/textView3"
            style="@style/second_normalTextView"
            android:layout_marginLeft="110px"
            android:text="重复Qr"
        />
        <TextView
            android:id="@+id/textView5"
            android:layout_below="@+id/textView4"
            style="@style/second_normalTextView"
            android:layout_marginLeft="110px"
            android:text="重复Kr"
        />
        <TextView
            android:id="@+id/textView6"
            android:layout_below="@+id/textView5"
            style="@style/second_normalTextView"
            android:layout_marginLeft="110px"
            android:text="均压Kp"
        />
        <TextView
            android:id="@+id/textView7"
            android:layout_below="@+id/textView6"
            style="@style/second_normalTextView"
            android:layout_marginLeft="110px"
            android:text="均压Ki"
        />
        <TextView
            android:id="@+id/textView8"
            android:layout_below="@+id/textView7"
            style="@style/second_normalTextView"
            android:layout_marginLeft="110px"
            android:text="电压Kp"
        />
        <TextView
            android:id="@+id/textView9"
            android:layout_below="@+id/textView8"
            style="@style/second_normalTextView"
            android:layout_marginLeft="110px"
            android:text="电压Ki"
        />
        <EditText
            android:id="@+id/zlUd"
            style="@style/input_EditView"
            android:layout_marginLeft="320px"
            android:layout_marginTop="120px"
        />
        <EditText
            android:id="@+id/cfQr"
            style="@style/input_EditView"
            android:layout_below="@+id/zlUd"
            android:layout_marginLeft="320px"
        />
        <EditText
            android:id="@+id/cfKr"
            style="@style/input_EditView"
            android:layout_below="@+id/cfQr"
            android:layout_marginLeft="320px"
        />
        <EditText
            android:id="@+id/jyKp"
            style="@style/input_EditView"
            android:layout_below="@+id/cfKr"
            android:layout_marginLeft="320px"
        />
        <EditText
            android:id="@+id/jyKi"
            style="@style/input_EditView"
            android:layout_below="@+id/jyKp"
            android:layout_marginLeft="320px"
        />
        <EditText
            android:id="@+id/dyKp"
            style="@style/input_EditView"
            android:layout_below="@+id/jyKi"
            android:layout_marginLeft="320px"
        />
        <EditText
            android:id="@+id/dyKi"
            style="@style/input_EditView"
            android:layout_below="@+id/dyKp"
            android:layout_marginLeft="320px"
         />

        <TextView
            android:id="@+id/textView15"
            style="@style/second_normalTextView"
            android:layout_marginTop="120px"
            android:layout_marginLeft="560px"
            android:text="电流Kp"
        />
        <TextView
            android:id="@+id/textView16"
            style="@style/second_normalTextView"
            android:layout_below="@+id/textView15"
            android:layout_marginLeft="560px"
            android:text="重复Td"
        />
        <TextView
            android:id="@+id/textView17"
            style="@style/second_normalTextView"
            android:layout_below="@+id/textView16"
            android:layout_marginLeft="560px"
            android:text="电压Rf"
        />
        <TextView
            android:id="@+id/textView18"
            style="@style/second_normalTextView"
            android:layout_below="@+id/textView17"
            android:layout_marginLeft="560px"
            android:text="过压DC"
        />
        <TextView
            android:id="@+id/textView19"
            style="@style/second_normalTextView"
            android:layout_below="@+id/textView18"
            android:layout_marginLeft="560px"
            android:text="过压AC"
        />
        <TextView
            android:id="@+id/textView20"
            style="@style/second_normalTextView"
            android:layout_below="@+id/textView19"
            android:layout_marginLeft="560px"
            android:text="欠压AC"
        />
        <TextView
            android:id="@+id/textView21"
            style="@style/second_normalTextView"
            android:layout_below="@+id/textView20"
            android:layout_marginLeft="560px"
            android:text="过温Tp"
        />


        <EditText
            android:id="@+id/dlKp"
            style="@style/input_EditView"
            android:layout_marginLeft="770px"
            android:layout_marginTop="120px"
        />

        <EditText
            android:id="@+id/cfTd"
            android:layout_below="@+id/dlKp"
            style="@style/input_EditView"
            android:layout_marginLeft="770px"
          />
        <EditText
            android:id="@+id/dyRf"
            android:layout_below="@+id/cfTd"
            style="@style/input_EditView"
            android:layout_marginLeft="770px"
        />
        <EditText
            android:id="@+id/gyDC"
            style="@style/input_EditView"
            android:layout_marginLeft="770px"
            android:layout_below="@+id/dyRf"
        />
        <EditText
            android:id="@+id/gyAC"
            style="@style/input_EditView"
            android:layout_marginLeft="770px"
            android:layout_below="@+id/gyDC"
          />
        <EditText
            android:id="@+id/qyAC"
            style="@style/input_EditView"
            android:layout_marginLeft="770px"
            android:layout_below="@+id/gyAC"
        />
        <EditText
            android:id="@+id/gwTp"
            style="@style/input_EditView"
            android:layout_marginLeft="770px"
            android:layout_below="@+id/qyAC"
        />
    </RelativeLayout>
    <RelativeLayout
        android:background="@drawable/the_shape2"
        android:layout_width="match_parent"
        android:layout_height="240px">
        <Button
            android:id="@+id/canshuhuichuan"
            android:layout_width="400px"
            android:layout_height="120px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="60px"
            android:background="@drawable/the_shape2"
            android:onClick="canshuhuichuan"
            android:text=" 参数回传 "
            android:textColor="@color/white"
            android:textSize="75px"/>
        <Button
            android:id="@+id/canshuxiugai"
            android:layout_width="400px"
            android:layout_height="120px"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="60px"
            android:background="@drawable/the_shape2"
            android:onClick="canshuxiugai"
            android:text=" 参数修改 "
            android:textColor="@color/white"
            android:textSize="75px"/>
        <TextView
            android:id="@+id/runstate"
            android:layout_width="120px"
            android:layout_height="90px"
            android:layout_centerInParent="true"
            android:background="@drawable/the_shape"
            android:gravity="center"
            android:text="停止"
            android:textColor="@android:color/white"
            android:textSize="50px"/>
    </RelativeLayout>


</LinearLayout>